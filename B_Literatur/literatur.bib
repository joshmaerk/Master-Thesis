@article{zhangHowWhenHumble2024,
  title = {How and When Humble Leadership Influences Employee Adaptive Performance? {{The}} Roles of Self-Determination and Employee Attributions},
  shorttitle = {How and When Humble Leadership Influences Employee Adaptive Performance?},
  author = {Zhang, Junting and Javaid, Mudaser and Liao, Shudi and Choi, Myeongcheol and Kim, Hann Earl},
