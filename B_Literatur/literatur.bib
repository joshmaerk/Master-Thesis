  keywords = {/journalA*,AIatWork,Automation,Context,DecisionSupport,Motivation,SDT,Supporting,WorkDesign},
  file = {/Users/joshuamarker/Zotero/storage/GWF5JBU7/Arokiyaswamy C et al. - 2024 - “An Empirical study on Enhancing Employee Engagement and Job Performance through Self-Determination.pdf}
}

