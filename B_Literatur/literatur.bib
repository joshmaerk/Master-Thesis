@article{meijerinkDualityAlgorithmicManagement2023,
  title = {The Duality of Algorithmic Management: {{Toward}} a Research Agenda on {{HRM}} Algorithms, Autonomy and Value Creation},
  shorttitle = {The Duality of Algorithmic Management},
